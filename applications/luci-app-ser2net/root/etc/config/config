config ser2net
    option enabled '0'
    option DeviceId '12345678'
    option SimNumber '13900001234'
    option interval '60'
    option heartbeat 'FE'
    option MaxDataLen '1000'
    option DataWaitTime '300'
    option ReDialTime '60'
    option serial '/dev/ttyS0'
    option 'baudrate' '57600'
    option 'databit' '8'
    option 'parity_check' 'n'
    option 'stopbit' '1'
    option srvmode '0'
    option srvport '9999'
    option clinum '5'
    option protocol1 'tcp'
    option ip1 ''
    option BasePort1 ''
    option protocol2 'tcp'
    option ip2 ''
    option BasePort2 ''
    option protocol3 'tcp'
    option protocol4 'tcp'
